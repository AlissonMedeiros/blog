{
  "name": "myapp",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "postbuild": "workbox inject:manifest",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "open": "http-server www  -o -a localhost -p 1234",
	"open-nocache": "http-server www -c-1 -o -a localhost -p 1234",
    "hashcb": "hashmark -l 8 -r --cwd www/build *.{js,css} {name}.{hash}{ext} | map-replace -m \"<[^>]+>\" www/index.html",
    "dist": "npm run clean && npm run build --prod && npm run hashcb && workbox inject:manifest"
  },
  "config": {
    "ionic_copy": "./copy.config.js"
  },
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@ionic/storage": "2.1.1",
    "ionic-angular": "3.8.0",
    "ionicons": "3.0.0",
    "rxjs": "5.5.2",
    "workbox-sw": "^2.1.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/tsc-wrapped": "^4.4.6",
    "@ionic/app-scripts": "3.0.1",
    "hashmark": "^5.0.0",
    "http-server": "^0.10.0",
    "map-replace": "^1.0.2",
    "typescript": "2.6.1",
    "workbox-cli": "^2.1.1"
  },
  "description": "An Ionic project"
}
